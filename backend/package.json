{
  "name": "@ai-blueprint/backend",
  "version": "1.0.0",
  "description": "Express.js backend for AI Web Full-Stack Blueprint",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --loader tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "test": "node --loader tsx --test src/**/*.test.ts",
    "test:watch": "node --watch --loader tsx --test src/**/*.test.ts",
    "test:coverage": "node --loader tsx --test --coverage src/**/*.test.ts",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "node --loader tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "generate": "prisma generate"
  },
  "dependencies": {
    "express": "^4.18.0",
    "typescript": "^5.3.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@prisma/client": "^5.0.0",
    "prisma": "^5.0.0",
    "zod": "^3.22.0",
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.0",
    "@types/bcryptjs": "^2.4.0",
    "dotenv": "^16.0.0",
    "cors": "^2.8.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "@types/morgan": "^1.9.0",
    "express-rate-limit": "^7.0.0",
    "axios": "^1.6.0",
    "bull": "^4.11.0",
    "@types/bull": "^3.15.0"
  },
  "devDependencies": {
    "tsx": "^4.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
